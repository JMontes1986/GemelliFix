
{
  "hosting": {
    "rewrites": [
      {
        "source": "/api/**",
        "function": "server"
      }
    ]
  },
  "functions": {
    "source": ".",
    "runtime": "nodejs18"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
